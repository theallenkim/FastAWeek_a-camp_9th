<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="${relativePackage}.${activityClass}"
    android:background="#EEEEEE" >
    
    <ImageView
    	android:id="@+id/inputCameraImageView"
    	android:layout_width="180dp"
    	android:layout_height="140dp"
    	android:background="#DDDDEE"
    	android:layout_centerHorizontal="true" />
    
    <TextView android:text="스크린샷을 통해 인식 된 문자" android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/textView2"
        android:textColor="#777777"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:layout_marginLeft="10dp"
        android:layout_marginTop="10dp"
        android:layout_below="@+id/inputCameraImageView" />

    <Button
        android:layout_width="60dp"
        android:layout_height="40dp"
        android:textColor="#FFFFFF"
        android:background="#49a0d7"
        android:text="스크린샷가져오기"
        android:id="@+id/startCamera"
        android:textSize="12dp"
        android:layout_above="@+id/textView3"
        android:layout_alignRight="@+id/tableLayout"
        android:layout_marginRight="5dp"
        android:layout_alignTop="@+id/cameraToStringTextView" 
        />

    <EditText
        android:layout_width="wrap_content"
        android:layout_height="40dp"
        android:id="@+id/cameraToStringTextView"
        android:background="#dbebed"
        android:textSize="16dp"
        android:singleLine="true"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:layout_alignParentRight="true"
        android:layout_alignParentEnd="true"
        android:layout_marginLeft="10dp"
        android:layout_marginRight="90dp"
        android:layout_below="@+id/textView2" />
    
        <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceLarge"
        android:text="@string/camera_input_comment_str"
        android:id="@+id/textView8"
        android:textSize="12dp"
        android:layout_below="@+id/cameraToStringTextView"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:layout_marginTop="5dp" 
        android:layout_marginLeft="10dp"
	/>
    
    <TextView
        android:text="분석 결과"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/textView3"
        android:layout_marginTop="42dp"
        android:layout_below="@+id/cameraToStringTextView"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:layout_marginLeft="10dp"
        />

    <TableLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_below="@+id/textView3"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:id="@+id/tableLayout"
        android:layout_marginTop="5dp"
        android:layout_marginBottom="60dp"
        android:layout_marginLeft="10dp"
        android:background="#e0e0e0">

        <TableRow
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_weight="0.3">

        </TableRow>

        <TableRow
            android:layout_width="fill_parent"
            android:layout_height="match_parent"
            android:layout_weight="1"
            >
			
            <TextView
                android:layout_width="0dp"
                android:layout_height="match_parent"
                android:text="날짜"
                android:id="@+id/textView4"
                android:layout_column="0"
                android:layout_weight="2"
                android:gravity="center"
                android:textColor="#FFFFFF"
                android:background="#49a0d7"
                android:layout_margin="10dp"
                />

            <EditText
                android:layout_width="0dp"
                android:layout_height="match_parent"
                android:id="@+id/camera_dateTextField"
                android:layout_column="1"
                android:background="#f5f5f5"
                android:editable="true"
                android:layout_weight="5"
                android:layout_span="0"
                android:singleLine="true"
                android:layout_margin="10dp"
                android:textSize="12dp" />
<!-- 
            <Button
                android:layout_width="0dp"
                android:layout_height="match_parent"
                android:text="달력"
                android:id="@+id/button4"
                android:layout_column="2"
                android:layout_weight="1"
                android:textSize="12dp"
                android:layout_gravity="center_vertical" />
      -->       
     <!--      </RelativeLayout>
      -->       
        </TableRow>

        <TableRow
            android:layout_width="fill_parent"
            android:layout_height="match_parent"
            android:layout_weight="0.2"
             />

        <TableRow
            android:layout_width="fill_parent"
            android:layout_height="match_parent"
            
            android:layout_weight="1">

            <TextView
                android:layout_width="0dp"
                android:layout_height="match_parent"
                android:text="시간"
                android:id="@+id/textView5"
                android:layout_column="0"
                android:layout_weight="2"
                android:gravity="center"
                android:background="#49a0d7"
                android:textColor="#FFFFFF"
                android:layout_margin="10dp"
                />

            <EditText
                android:layout_width="0dp"
                android:layout_height="match_parent"
                android:id="@+id/camera_timeTextField"
                android:layout_column="1"
                android:background="#f5f5f5"
                android:editable="true"
                android:layout_weight="5"
                android:singleLine="true"
                android:layout_span="0" 
                android:layout_margin="10dp"
                android:textSize="12dp"/>
<!-- 
            <Button
                android:layout_width="0dp"
                android:layout_height="match_parent"
                android:text="시간"
                android:id="@+id/button5"
                android:layout_column="2"
                android:textSize="12dp"
                android:layout_weight="1"
                android:layout_gravity="center_vertical" />
 -->
        </TableRow>

        <TableRow
            android:layout_width="fill_parent"
            android:layout_height="match_parent"
            android:layout_weight="0.2"
            />

        <TableRow
            android:layout_width="fill_parent"
            android:layout_height="match_parent"
            
            android:layout_weight="1">

            <TextView
                android:layout_width="0dp"
                android:layout_height="match_parent"
                android:text="장소"
                android:id="@+id/textView6"
                android:layout_column="0"
                android:layout_weight="2"
                android:gravity="center"
                android:background="#49a0d7"
                android:textColor="#FFFFFF"
                android:layout_margin="10dp"
                />

            <EditText
                android:layout_width="0dp"
                android:layout_height="match_parent"
                android:id="@+id/camera_whereTextField"
                android:layout_column="1"
                android:autoText="false"
                android:background="#f5f5f5"
                android:editable="true"
                android:layout_weight="5"
                android:singleLine="true"
                android:layout_span="0"
                android:layout_margin="10dp"
                android:textSize="12dp" />
<!--
            <Button
                android:layout_width="0dp"
                android:layout_height="match_parent"
                android:text="위치"
                android:id="@+id/button6"
                android:layout_column="2"
                android:layout_weight="1"
                android:textSize="12dp"
                android:layout_gravity="center_vertical" />
-->
        </TableRow>

        <TableRow
            android:layout_width="fill_parent"
            android:layout_height="match_parent"
            android:layout_weight="0.2"
             />

        <TableRow
            android:layout_width="fill_parent"
            android:layout_height="match_parent"
            android:layout_weight="1"
            >

            <TextView
                android:layout_width="0dp"
                android:layout_height="match_parent"
                android:text="누구랑"
                android:id="@+id/textView7"
                android:layout_column="0"
                android:layout_weight="2"
                android:gravity="center"
                android:background="#49a0d7"
                android:textColor="#FFFFFF"
                android:layout_margin="10dp"
                />

            <EditText
                android:layout_width="0dp"
                android:layout_height="match_parent"
                android:id="@+id/camera_whoTextField"
                android:layout_column="1"
                android:background="#f5f5f5"
                android:editable="true"
                android:layout_weight="5"
                android:singleLine="true"
                android:layout_span="0" 
                android:layout_margin="10dp"
                android:textSize="12dp" 
                />
<!-- 
            <Button
                android:layout_width="0dp"
                android:layout_height="match_parent"
                android:text="연락처"
                android:id="@+id/button7"
                android:layout_column="2"
                android:textSize="12dp"
                android:layout_weight="1"
                android:layout_gravity="center_vertical" />
 -->
        </TableRow>

        <TableRow
            android:layout_width="fill_parent"
            android:layout_height="match_parent"
            android:layout_weight="0.2"
            ></TableRow>

        <TableRow
            android:layout_width="fill_parent"
            android:layout_height="match_parent"
            android:layout_weight="2"
            android:textAlignment="center">

            <TextView
                android:layout_width="0dp"
                android:layout_height="match_parent"
                android:text="인식 되지 않은 음성 내용"
                android:id="@+id/textView9"
                android:layout_column="0"
                android:lines="2"
                android:maxLines="2"
                android:layout_weight="1.2"
                android:gravity="center"
                android:background="#49a0d7"
                android:textColor="#FFFFFF"
                android:layout_margin="10dp"
                />

            <EditText
                android:layout_width="0dp"
                android:layout_height="match_parent"
                android:id="@+id/camera_otherText"
                android:layout_column="1"
                android:background="#dcfffa"
                android:editable="true"
                android:maxLines="3"
                android:layout_span="0"
                android:layout_weight="3" 
                android:layout_margin="10dp"
                android:textSize="12dp" />
        </TableRow>

    </TableLayout>


<LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_alignLeft="@+id/tableLayout"
        android:layout_alignParentBottom="true"
        android:layout_alignRight="@+id/tableLayout"
        android:layout_marginTop="10dp"
        android:layout_marginBottom="10dp"
        android:layout_marginLeft="20dp" 
        android:layout_marginRight="20dp">

        <Button
            android:id="@+id/camera_submit"
            android:layout_width="120dp"
            android:layout_height="40dp"
            android:layout_weight="1"
            android:background="#49a0d7"
            android:textColor="#FFFFFF"
            android:text="일정등록" />
        
        <Button
            android:id="@+id/camera_cancel"
            android:layout_width="120dp"
            android:layout_height="40dp"
            android:layout_weight="1"
            android:background="#AAAAAA"
            android:textColor="#000000"
            android:text="취소" />

        
    </LinearLayout>


</RelativeLayout>
